{
  "name": "tina4stack/tina4php-mongodb",
  "description": "Tina4 PHP MongoDB Database Driver",
  "license": "MIT",
  "autoload": {
    "classmap": [
      "Tina4/"
    ],
    "psr-4": {
      "Tina4\\": [
        "Tina4/"
      ]
    }
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^9",
    "overtrue/phplint": "^2.0"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit tests --color --log-junit=tests/junit.xml",
    "phpmetrics": "phpmetrics --report-html=phpmetrics --junit=tests/junit.xml ./Tina4"
  },
  "require": {
    "tina4stack/tina4php-database": "^2.0",
    "ext-mongodb" : "*",
    "mongodb/mongodb": "^1.17"
  }
}
